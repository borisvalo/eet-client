{
	"name": "slevomat/eet-client",
	"description": "Client for electronic records of sales (EET) in Czech republic",
	"license": "MIT",
	"type": "library",
	"keywords": ["eet", "electronic records of sales"],
	"authors": [
		{
			"name": "Slevomat.cz, s.r.o.",
			"homepage": "http://www.slevomat.cz"
		}
	],
	"config": {
		"sort-packages": true
	},
	"support": {
		"issues": "https://github.com/slevomat/eet-client/issues"
	},
	"require": {
		"php": "^7.0",
		"ext-openssl": "*",
		"ext-soap": "*",
		"consistence/consistence": "^1.0.0",
		"ramsey/uuid": "^3.5",
		"robrichards/wse-php": "^2.0"
	},
	"require-dev": {
		"composer/ca-bundle": "^1.0",
		"consistence/coding-standard": "^2.0.0",
		"guzzlehttp/guzzle": "^6.2",
		"jakub-onderka/php-parallel-lint": "^0.9.2",
		"phing/phing": "^2.16",
		"phpstan/phpstan": "^0.8.5",
		"phpunit/phpunit": "^6.2.0",
		"slevomat/coding-standard": "^4.0.0"
	},
	"autoload": {
		"psr-4": {
			"SlevomatEET\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"SlevomatEET\\": "tests/SlevomatEET"
		}
	},
	"suggest": {
		"guzzlehttp/guzzle": "For usage of bundled GuzzleSoapClientDriver"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.x-dev"
		}
	}
}
